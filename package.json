{
  "name": "@medinovai/demo-data",
  "version": "1.0.0",
  "description": "Intelligent demo data generation system for healthcare applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "generate": "ts-node src/cli.ts generate",
    "validate": "ts-node src/cli.ts validate",
    "import": "ts-node src/cli.ts import",
    "export": "ts-node src/cli.ts export",
    "scenario": "ts-node src/cli.ts scenario",
    "docker:build": "docker build -t medinovai/demo-data .",
    "docker:run": "docker run -it medinovai/demo-data"
  },
  "keywords": [
    "demo-data",
    "healthcare",
    "data-generation",
    "testing",
    "synthetic-data"
  ],
  "author": "MedinovaAI Team",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "yargs": "^17.7.2",
    "js-yaml": "^4.1.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "exceljs": "^4.4.0",
    "uuid": "^9.0.1",
    "bcrypt": "^5.1.1",
    "jsonschema": "^1.4.1",
    "p-queue": "^7.4.1",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "inquirer": "^10.0.0",
    "handlebars": "^4.7.8",
    "bull": "^4.11.5",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.201",
    "@types/bcrypt": "^5.0.2",
    "@types/uuid": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/pg": "^8.10.9",
    "@types/inquirer": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.test.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
} 